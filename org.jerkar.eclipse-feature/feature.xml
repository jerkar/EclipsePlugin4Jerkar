<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.jerkar.eclipse_feature"
      label="Jerkar IDE"
      version="0.1.2"
      provider-name="org.jerkar">

   <description>
      Add contextual menu to project view, in order to launch Jerkar commands from here.
   </description>

   <copyright>
      jerkar.org Open Source
Copyright individual contributors as indicated by the @authors tag, 2014.
   </copyright>

   <license url="http://www.apache.org/licenses/LICENSE-2.0">
      Apache 2 License.
   </license>

   <requires>
      <import plugin="org.eclipse.ui" version="3.105.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.9.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.8.101" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.ui" version="3.9.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.9.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.console" version="3.5.200" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.jerkar.eclipse-plugin"
         download-size="0"
         install-size="0"
         version="0.1.2"
         unpack="false"/>

</feature>
